{
    "name": "erkurn/pedia",
    "description": "Form Fieldsn Generator By Backend Code Driven",
    "keywords": [
        "erkurn",
        "pedia"
    ],
    "homepage": "https://github.com/erkurn/pedia",
    "license": "MIT",
    "authors": [
        {
            "name": "Rezza Kurniawan",
            "email": "rezza.kurniawan@gmail.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Erkurn\\Pedia\\": "src",
            "Erkurn\\Pedia\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Erkurn\\Pedia\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Erkurn\\Pedia\\PediaServiceProvider"
            ],
            "aliases": {
                "Pedia": "Erkurn\\Pedia\\PediaFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/spatie"
        },
        {
            "type": "other",
            "url": "https://spatie.be/open-source/support-us"
        }
    ]
}
